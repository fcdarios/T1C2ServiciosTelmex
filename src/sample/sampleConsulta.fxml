<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">

    <GridPane id="" alignment="center" hgap="10" vgap="10" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">

    <JFXTextField fx:id="textField1" prefHeight="40" prefWidth="80" promptText="Numero Telefonico" GridPane.columnIndex="0" GridPane.columnSpan="3" GridPane.rowIndex="0" />

    <JFXButton fx:id="BtnConsultar" buttonType="RAISED" style="-fx-background-color: dodgerblue" prefHeight="40" prefWidth="100" text="Consultar" GridPane.columnIndex="3" GridPane.rowIndex="0">
        <graphic>
            <FontAwesomeIconView fill="black" glyphName="SEARCH" glyphSize="20" />
        </graphic>
    </JFXButton>

    <JFXButton fx:id="BtnCancelar" buttonType="RAISED" style="-fx-background-color: dodgerblue" prefHeight="40" prefWidth="100" text="Cancelar" GridPane.columnIndex="3" GridPane.rowIndex="1">
        <graphic>
            <FontAwesomeIconView fill="black" glyphName="CLOSE" glyphSize="20" />
        </graphic>
    </JFXButton>

    <JFXButton fx:id="BtnAyuda" buttonType="RAISED" style="-fx-background-color: dodgerblue" prefHeight="40" prefWidth="100" text="Ayuda" GridPane.columnIndex="3" GridPane.rowIndex="2">
        <graphic>
            <FontAwesomeIconView fill="black" glyphName="QUESTION" glyphSize="20" />
        </graphic>
    </JFXButton>

    <Button fx:id="Button1" prefHeight="40" prefWidth="50" styleClass="btn, btn-default" text="1" GridPane.columnIndex="0" GridPane.rowIndex="1" />
    <Button fx:id="Button2" prefHeight="40" prefWidth="50" styleClass="btn, btn-default" text="2" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    <Button fx:id="Button3" prefHeight="40" prefWidth="50" styleClass="btn, btn-default" text="3" GridPane.columnIndex="2" GridPane.rowIndex="1" />
    <Button fx:id="Button4" prefHeight="40" prefWidth="50" styleClass="btn, btn-default" text="4" GridPane.columnIndex="0" GridPane.rowIndex="2" />
    <Button fx:id="Button5" prefHeight="40" prefWidth="50" styleClass="btn, btn-default" text="5" GridPane.columnIndex="1" GridPane.rowIndex="2" />
    <Button fx:id="Button6" prefHeight="40" prefWidth="50" styleClass="btn, btn-default" text="6" GridPane.columnIndex="2" GridPane.rowIndex="2" />
    <Button fx:id="Button7" prefHeight="40" prefWidth="50" styleClass="btn, btn-default" text="7" GridPane.columnIndex="0" GridPane.rowIndex="3" />
    <Button fx:id="Button8" prefHeight="40" prefWidth="50" styleClass="btn, btn-default" text="8" GridPane.columnIndex="1" GridPane.rowIndex="3" />
    <Button fx:id="Button9" prefHeight="40" prefWidth="50" styleClass="btn, btn-default" text="9" GridPane.columnIndex="2" GridPane.rowIndex="3" />
    <Button fx:id="Button10" prefHeight="40" prefWidth="165" styleClass="btn, btn-default" text="0" GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.rowIndex="4" />
    <Button fx:id="BtnBorrar" prefHeight="40" prefWidth="50" styleClass="btn, btn-default" text="" GridPane.columnIndex="2" GridPane.rowIndex="4">
        <graphic>
            <FontAwesomeIconView fill="black" glyphName="ARROW_CIRCLE_LEFT" glyphSize="20" />
        </graphic>
    </Button>
      <columnConstraints>
         <ColumnConstraints />
         <ColumnConstraints />
         <ColumnConstraints />
         <ColumnConstraints />
      </columnConstraints>
      <rowConstraints>
         <RowConstraints />
         <RowConstraints />
